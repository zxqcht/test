#include <stdio.h>
void reverse(int n)
{ 
  int i,z=0,x,q;
  scanf("%d",&n);
  	if(n%10==0)
    {
      while(n>0){
  	    z=n%10;
  	    printf("%d",z);
  	    n/=10;
	  }
	}
	if(n%10!=0)
	{ 
	  while(n!=0){
  	    z=z*10+n%10;
  	    n/=10;
	  }
	  printf("%d",z); 
}
}
  
int main() 
{ 
  int x;
  reverse(x);
}
