#include <stdio.h>
#include <stdlib.h>
#include <string.h>
typedef struct node{
	int data;
	node *next;
}list;
int main(){
	struct node *head,*p,*q,*temp;
	int i,j,n,a,b;
	scanf("%d",&n);
	head=NULL;
	for(i=0;i<n;i++){
		scanf("%d",&a);
		p=(struct node*)malloc(sizeof(node));
		p->data=a;
		p->next=NULL;
		if(head==NULL){
		   head=p;
		} 
		else{
		   temp->next=p;
		}
		temp=p;
	}
	q=head;
	while(q!=NULL){
		printf("%d",q->data);
		q=q->next;
	}
}
